            <option name='RESOLVE_MATH_REF' value='true'/>
        </context>
    </template>

    <template description='≤' name='leq' toReformat='true' toShortenFQNames='true'
              value='≤ $END$'>
        <context>
            <option name='RESOLVE_MATH_REF' value='true'/>
        </context>
    </template>

</templateSet>