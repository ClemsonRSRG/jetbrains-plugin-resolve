<?xml version='1.0' encoding='UTF-8'?>

<templateSet group="ResolveHiddenTemplate">

    <template description='Record type' id='resolve_lang_Record' name='resolve_lang_Record' toReformat='true' toShortenFQNames='true'
              value='Record&#10;        $END$&#10;    end'>
        <context>
            <option name='RESOLVE' value='true'/>
        </context>
    </template>

    <template description='Cart prod type' id='resolve_lang_Cart_Prod' name='resolve_lang_Cart_Prod' toReformat='true' toShortenFQNames='true'
