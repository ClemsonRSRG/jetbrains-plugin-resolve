Facility Function_Arguments;

Operation Add(evaluates x : Std_Ints.Integer;
              evaluates y : Std_Ints.Integer) : Std_Ints.Integer;
Procedure Add := x + y; end Add;

Operation Add(evaluates x, y : Std_Ints.Integer) : Std_Ints.Integer;
Procedure Add := x + y; end Add;

end Function_Arguments;