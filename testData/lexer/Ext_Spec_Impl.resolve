Implementation Ext_Spec_Impl for Ext_Spec of Stack_Spec;
	Procedure Flip(updates S : Stack);
		Var Temp : Stack;
		Var Next_Entry : Entry;

		While 1 <= Depth(S)
			maintaining @S = Reverse(Temp) o S;
			decreasing |S|;
		do
			Pop(Next_Entry, S);
			Push(Next_Entry, Temp);
		end;
		Temp :=: S;
	end Flip;
end Ext_Spec_Impl;